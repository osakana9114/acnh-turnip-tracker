{
  "name": "with-aws-amplify-typescript",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "stylelint": {
    "extends": [
      "stylelint-prettier/recommended",
      "stylelint-config-recess-order"
    ],
    "syntax": "scss",
    "plugins": [
      "stylelint-scss",
      "stylelint-order",
      "stylelint-prettier"
    ],
    "rules": {
      "prettier/prettier": true,
      "order/order": [
        [
          "dollar-variables",
          "declarations",
          "rules"
        ]
      ],
      "color-hex-length": "short",
      "length-zero-no-unit": true
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "react-app",
      "airbnb-base",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "import"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": [
    "last 1 version",
    "> 3% in JP",
    "ie >= 11",
    "iOS >= 10",
    "Android >= 5.1"
  ],
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "aws-amplify": "^2.3.0",
    "chart.js": "^2.9.3",
    "date-fns": "^2.14.0",
    "eslint": "^7.0.0",
    "immer": "3.1.3",
    "modern-css-reset": "^1.1.0",
    "nanoid": "2.0.3",
    "next": "^9.0.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass": "^1.26.5",
    "sass-mq": "^5.0.1",
    "stylelint": "^13.4.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.17.2",
    "what-input": "^5.2.9"
  },
  "devDependencies": {
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "typescript": "3.5.3"
  }
}
