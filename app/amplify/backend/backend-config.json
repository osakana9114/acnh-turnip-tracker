{
	"hosting": {
		"amplifyhosting": {
			"service": "amplifyhosting",
			"providerPlugin": "awscloudformation",
			"type": "manual"
		}
	},
	"api": {
		"exampletodo": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "API_KEY",
						"apiKeyConfig": {
							"description": "default todo list sample",
							"apiKeyExpirationDays": "7"
						}
					}
				}
			}
		}
	}
}